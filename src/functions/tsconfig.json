{
	"compilerOptions": {
		"lib": ["es6", "dom", "es2016"],
		"module": "commonjs",
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"outDir": "../../dist/functions",
		"sourceMap": true,
		"skipLibCheck": true,
		"strict": true,
		"target": "es2017",
		"baseUrl": "./",
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"isolatedModules": true
	},
	"compileOnSave": true,
	"include": ["src"]
}
